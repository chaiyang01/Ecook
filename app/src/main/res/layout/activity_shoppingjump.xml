<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:id="@+id/shopping_relative_lay_out"
        android:layout_width="match_parent"
        android:layout_height="45dp">
        <TextView
            android:id="@+id/shopping_tv_back_show"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="返回"
            android:gravity="center"
            android:layout_centerVertical="true"
            android:drawableLeft="@drawable/topbanner_white_arrow"
            android:clickable="true"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="厨币商城"
            android:textSize="20sp"
            android:textStyle="bold"
            android:layout_centerInParent="true"
            />
        <ImageView
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:src="@drawable/shop_mine"
            android:scaleType="fitXY"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp" />
    </RelativeLayout>
    
    <WebView
        android:id="@+id/shopping_jump_webview"
        android:layout_below="@id/shopping_relative_lay_out"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none">

    </WebView>
</RelativeLayout>